{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/index.csr.html",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/**/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-requests",
      "urls": ["/api/**"],
      "cacheConfig": {
        "strategy": "freshness",
        "maxAge": "0u",
        "timeout": "0u"
      }
    },
    {
      "name": "article-feed-data",
      "urls": [
        "/api/trpc/article.list*",
        "/api/trpc/feed.list*",
        "/api/trpc/article.getById*",
        "/api/trpc/feed.getById*"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxAge": "2m",
        "timeout": "10s",
        "maxEntries": 50
      }
    },
    {
      "name": "image-proxy",
      "urls": ["/api/image-proxy*"],
      "cacheConfig": {
        "strategy": "performance",
        "maxAge": "7d",
        "timeout": "30s",
        "maxEntries": 200
      }
    }
  ]
}
